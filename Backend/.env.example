# API Security
API_KEY=your-secret-api-key-change-this

# aria2 Configuration
ARIA2_RPC_SECRET=change-me-aria2-secret
ARIA2_MAX_CONNECTIONS=16
ARIA2_SPLIT=16
ARIA2_MAX_CONCURRENT_DOWNLOADS=5

# WebDAV Configuration (Nextcloud)
NEXTCLOUD_USERNAME=your_username
NEXTCLOUD_PASSWORD=your_password
WEBDAV_URL=https://your-nextcloud.com/remote.php/dav/files/your_username/

# System Configuration
MAX_FILE_SIZE_GB=5
CELERY_WORKER_CONCURRENCY=2
DOWNLOAD_TIMEOUT=3600
MAX_RETRIES=3